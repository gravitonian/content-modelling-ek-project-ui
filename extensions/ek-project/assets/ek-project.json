{
  "$schema": "../../../extension.schema.json",
  "$name": "EK Project Extension",
  "$version": "1.0.0",
  "$description": "ACME Extension for EK Project with Client/Supplier functionality",
  "$vendor": "ACME",
  "$license": "MIT",
  "$dependencies": {
    "$all": ["app.extension"]
  },

  "routes": [],

  "actions": [
    {
      "id": "ek-project.actions.take.ownership",
      "type": "TAKE_OWNERSHIP",
      "payload": "$(context.selection.first.entry)"
    },
    {
      "id": "ek-project.actions.set-title-description",
      "type": "SET_TITLE_DESCRIPTION",
      "payload": "$(context.selection.first.entry)"
    },
    {
      "id": "ek-project.actions.create-client-supplier",
      "type": "CREATE_CLIENT_SUPPLIER"
    }
  ],

  "rules": [],

  "features": {
    "search": [
      {
        "id": "ek-project.search.ekDocuments",
        "order": 402    ,
        "name": "EK_PROJECT.SEARCH.EK_DOCUMENTS",
        "default": false,
        "filterWithContains": true,
        "app:fields": ["cm:name", "cm:title", "cm:description", "ek:docId","TEXT", "TAG"],
        "include": ["path", "allowableOperations", "properties"],
        "sorting": {
          "options": [
            {
              "key": "score",
              "label": "SEARCH.SORT.RELEVANCE",
              "type": "SCORE",
              "field": "score",
              "ascending": false
            },
            {
              "key": "name",
              "label": "SEARCH.SORT.FILENAME",
              "type": "FIELD",
              "field": "cm:name",
              "ascending": true
            },
            {
              "key": "title",
              "label": "SEARCH.SORT.TITLE",
              "type": "FIELD",
              "field": "cm:title",
              "ascending": true
            },
            {
              "key": "created",
              "label": "SEARCH.SORT.CREATE_DATE",
              "type": "FIELD",
              "field": "cm:created",
              "ascending": true
            }
          ],
          "defaults": [
            {
              "key": "score",
              "type": "SCORE",
              "field": "score",
              "ascending": false
            }
          ]
        },
        "aca:triggeredOnChange": false,
        "resetButton": true,
        "filterQueries": [
          { "query": "+TYPE:'ek:documentBase'" },
          { "query": "-TYPE:'cm:thumbnail' AND -TYPE:'cm:failedThumbnail' AND -TYPE:'cm:rating'" },
          { "query": "-cm:creator:System" },
          { "query": "-TYPE:'st:site' AND -ASPECT:'st:siteContainer' AND -ASPECT:'sys:hidden'" },
          { "query": "-TYPE:'dl:dataList' AND -TYPE:'dl:todoList' AND -TYPE:'dl:issue'" },
          { "query": "-TYPE:'fm:topic' AND -TYPE:'fm:post'" },
          { "query": "-TYPE:'lnk:link'" },
          { "query": "-PATH:'//cm:wiki/*'" }
        ],
        "facetFields": {
          "expanded": true,
          "fields": [
            {
              "mincount": 1,
              "field": "creator",
              "label": "SEARCH.FACET_FIELDS.CREATOR",
              "settings": {
                "allowUpdateOnChange": false,
                "hideDefaultAction": true
              }
            },
            {
              "mincount": 1,
              "field": "modifier",
              "label": "SEARCH.FACET_FIELDS.MODIFIER",
              "settings": {
                "allowUpdateOnChange": false,
                "hideDefaultAction": true
              }
            }
          ]
        },
        "categories": [
          {
            "id": "logic",
            "name": "SEARCH.CATEGORIES.LOGIC",
            "enabled": true,
            "component": {
              "selector": "logical-filter",
              "settings": {
                "allowUpdateOnChange": false,
                "hideDefaultAction": true,
                "field": "cm:name,cm:title,cm:description,ek:docId,TEXT,TAG"
              }
            }
          },
          {
            "id": "documentType",
            "name": "EK_PROJECT.SEARCH.CATEGORIES.DOCUMENT_TYPE",
            "enabled": true,
            "component": {
              "selector": "check-list",
              "settings": {
                "allowUpdateOnChange": true,
                "hideDefaultAction": true,
                "field": "TYPE",
                "options": [
                  { "name": "Corporate Policy", "value": "TYPE:'ek:corporatePolicyDocument'" },
                  { "name": "Procedure Document", "value": "TYPE:'ek:procedureDocument'" },
                  { "name": "Contract Document", "value": "TYPE:'ek:contractDocument'" },
                  { "name": "Compliance Report", "value": "TYPE:'ek:complianceReportDocument'" }
                ]
              }
            }
          },
          {
            "id": "createdModifiedDateRange",
            "name": "SEARCH.CATEGORIES.DATE",
            "enabled": true,
            "component": {
              "selector": "date-range",
              "settings": {
                "allowUpdateOnChange": false,
                "hideDefaultAction": true,
                "field": "cm:created,cm:modified",
                "dateFormat": "dd-MMM-yy",
                "maxDate": "today",
                "displayedLabelsByField": {
                  "cm:created": "SEARCH.CATEGORIES.DATE_TABS.CREATED",
                  "cm:modified": "SEARCH.CATEGORIES.DATE_TABS.MODIFIED"
                }
              }
            }
          }
        ],
        "highlight": {
          "prefix": "<span class='aca-highlight'>",
          "postfix": "</span>",
          "fields": [
            {
              "field": "cm:title"
            },
            {
              "field": "cm:name"
            },
            {
              "field": "cm:description",
              "snippetCount": 1
            },
            {
              "field": "cm:content",
              "snippetCount": 1
            }
          ]
        }
